{
  "name": "personal-nuxt-template",
  "description": "My personal template for nuxt projects",
  "author": "EliasSchaut",
  "private": true,
  "type": "commonjs",
  "license": "MIT",
  "homepage": "https://github.com/EliasSchaut/NuxtTemplate#README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/EliasSchaut/NuxtTemplate.git"
  },
  "scripts": {
    "start": "npx nuxt start",
    "dev:client": "npx nuxt dev --cwd . client",
    "dev:server": "npx nest start --debug --watch",
    "test:server": "jest --config=jest.config.json",
    "build": "npm run build:server && npm run build:client && npm run build:db",
    "build:server": "nest build",
    "build:client": "nuxt build",
    "build:db": "npx prisma migrate dev --schema=./schema.prisma",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nuxt/fonts": "^0.11.2",
    "@nuxt/image": "^1.10.0",
    "@nuxtjs/apollo": "^5.0.0-alpha.14",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/i18n": "^9.5.4",
    "@nuxtjs/sitemap": "^7.2.10",
    "@pinia/nuxt": "^0.11.0",
    "@prisma/client": "^6.7.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.5",
    "@vueuse/core": "^13.1.0",
    "@vueuse/motion": "^3.0.3",
    "@vueuse/nuxt": "^13.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "generate-password": "^1.7.1",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "nestjs-i18n": "^10.5.1",
    "nestjs-prisma": "^0.25.0",
    "nodemailer": "^7.0.2",
    "nuxt": "^3.17.2",
    "nuxt-particles": "^0.3.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "prisma": "^6.7.0",
    "prisma-error-enum": "^0.1.3",
    "reflect-metadata": "^0.2.2",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3",
    "typewriter-effect": "^2.22.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.0",
    "@nuxt/devtools": "^2.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/nodemailer": "^6.4.17",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
